{
  "name": "camera-node-addons",
  "version": "1.0.0",
  "description": "相机node c++原生插件",
  "keywords": [
    "c++ addons",
    "node",
    "camera",
    "twain"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "nongcundeshifu",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.js",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "yarn pub",
    "configure": "node-gyp configure",
    "addons-build": "node-gyp build",
    "addons-rebuild": "node-gyp rebuild",
    "addons-build-debug": "node-gyp build --debug",
    "addons-rebuild-debug": "node-gyp rebuild --debug"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "fs-extra": "^9.1.0",
    "node-addon-api": "^4.3.0"
  },
  "devDependencies": {
    "@types/bindings": "^1.5.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.6.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
